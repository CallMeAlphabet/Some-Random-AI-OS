{
  "categories": {
    "file_system_basic": {
      "name": "Basic File System Operations",
      "commands": ["ls", "cd", "pwd", "mkdir", "rmdir", "touch", "rm", "cp", "mv", "ln"]
    },
    "file_viewing_editing": {
      "name": "File Viewing and Editing",
      "commands": ["cat", "head", "tail", "less", "nano", "vim", "echo"]
    },
    "file_search_manipulation": {
      "name": "File Search and Manipulation",
      "commands": ["find", "grep", "sed", "awk", "wc", "sort", "uniq", "diff"]
    },
    "file_permissions_ownership": {
      "name": "File Permissions and Ownership",
      "commands": ["chmod", "chown"]
    },
    "compression_archiving": {
      "name": "Compression and Archiving",
      "commands": ["tar", "gzip", "gunzip", "zip", "unzip"]
    },
    "system_information": {
      "name": "System Information",
      "commands": ["uname", "hostname", "whoami", "id", "uptime", "date", "cal", "df", "du", "free"]
    },
    "process_management": {
      "name": "Process Management",
      "commands": ["ps", "top", "htop", "kill", "killall", "jobs", "bg", "fg", "nohup"]
    },
    "networking": {
      "name": "Networking",
      "commands": ["ping", "curl", "wget", "netstat", "ifconfig", "ip", "ssh", "scp", "browser"]
    },
    "package_management": {
      "name": "Package Management",
      "commands": ["pkg"]
    },
    "development_tools": {
      "name": "Development Tools",
      "commands": ["git", "python3", "node", "gcc", "make"]
    },
    "applications": {
      "name": "Applications",
      "commands": ["calculator"]
    },
    "shell_features": {
      "name": "Shell Features",
      "commands": ["history", "alias", "unalias", "export", "env", "source", "clear", "exit", "help", "man"]
    }
  },
  "commands": {
    "ls": {
      "description": "List directory contents",
      "category": "file_system_basic",
      "usage": "ls [OPTIONS] [FILE...]",
      "options": {
        "-l": "Use long listing format",
        "-a": "Show hidden files (starting with .)",
        "-h": "Human-readable sizes",
        "-R": "Recursive listing",
        "-t": "Sort by modification time",
        "-r": "Reverse sort order",
        "-S": "Sort by file size",
        "-1": "One file per line"
      },
      "examples": [
        "ls",
        "ls -la",
        "ls -lh /home/user",
        "ls -lR",
        "ls -lt"
      ]
    },
    "cd": {
      "description": "Change directory",
      "category": "file_system_basic",
      "usage": "cd [DIRECTORY]",
      "special_paths": {
        "~": "Home directory",
        "-": "Previous directory",
        "..": "Parent directory",
        ".": "Current directory",
        "/": "Root directory"
      },
      "examples": [
        "cd /home/user/Documents",
        "cd ~",
        "cd ..",
        "cd -",
        "cd"
      ]
    },
    "pwd": {
      "description": "Print working directory",
      "category": "file_system_basic",
      "usage": "pwd [OPTIONS]",
      "options": {
        "-L": "Print logical path (with symlinks)",
        "-P": "Print physical path (without symlinks)"
      },
      "examples": ["pwd"]
    },
    "mkdir": {
      "description": "Create directory",
      "category": "file_system_basic",
      "usage": "mkdir [OPTIONS] DIRECTORY...",
      "options": {
        "-p": "Create parent directories as needed",
        "-v": "Verbose output",
        "-m MODE": "Set permissions"
      },
      "examples": [
        "mkdir newfolder",
        "mkdir -p path/to/deep/folder",
        "mkdir dir1 dir2 dir3",
        "mkdir -m 755 public_folder"
      ]
    },
    "rmdir": {
      "description": "Remove empty directory",
      "category": "file_system_basic",
      "usage": "rmdir [OPTIONS] DIRECTORY...",
      "options": {
        "-p": "Remove parent directories if empty",
        "-v": "Verbose output"
      },
      "examples": [
        "rmdir emptyfolder",
        "rmdir -p path/to/empty"
      ]
    },
    "touch": {
      "description": "Create empty file or update timestamp",
      "category": "file_system_basic",
      "usage": "touch [OPTIONS] FILE...",
      "options": {
        "-a": "Change only access time",
        "-m": "Change only modification time",
        "-c": "Do not create file if it doesn't exist",
        "-t STAMP": "Use specific timestamp"
      },
      "examples": [
        "touch newfile.txt",
        "touch file1.txt file2.txt",
        "touch -t 202501011200 oldfile.txt"
      ]
    },
    "rm": {
      "description": "Remove files or directories",
      "category": "file_system_basic",
      "usage": "rm [OPTIONS] FILE...",
      "options": {
        "-r": "Remove directories recursively",
        "-f": "Force removal without prompts",
        "-i": "Prompt before removal",
        "-v": "Verbose output",
        "-d": "Remove empty directories"
      },
      "examples": [
        "rm file.txt",
        "rm -r folder/",
        "rm -rf folder/",
        "rm *.tmp",
        "rm -i important.txt"
      ]
    },
    "cp": {
      "description": "Copy files or directories",
      "category": "file_system_basic",
      "usage": "cp [OPTIONS] SOURCE DEST",
      "options": {
        "-r": "Copy directories recursively",
        "-i": "Prompt before overwrite",
        "-v": "Verbose output",
        "-p": "Preserve file attributes",
        "-u": "Copy only when source is newer",
        "-a": "Archive mode (preserve all)"
      },
      "examples": [
        "cp file.txt backup.txt",
        "cp -r folder/ backup/",
        "cp *.txt Documents/",
        "cp -a project/ project_backup/"
      ]
    },
    "mv": {
      "description": "Move or rename files",
      "category": "file_system_basic",
      "usage": "mv [OPTIONS] SOURCE DEST",
      "options": {
        "-i": "Prompt before overwrite",
        "-v": "Verbose output",
        "-f": "Force overwrite",
        "-n": "Do not overwrite existing",
        "-u": "Move only when source is newer"
      },
      "examples": [
        "mv oldname.txt newname.txt",
        "mv file.txt Documents/",
        "mv *.log /var/log/",
        "mv -i important.txt backup/"
      ]
    },
    "ln": {
      "description": "Create links between files",
      "category": "file_system_basic",
      "usage": "ln [OPTIONS] TARGET LINK_NAME",
      "options": {
        "-s": "Create symbolic (soft) link",
        "-f": "Force creation",
        "-v": "Verbose output",
        "-n": "Treat destination as normal file if it's a symlink"
      },
      "examples": [
        "ln -s /path/to/file link",
        "ln file hardlink",
        "ln -sf /usr/bin/python3 /usr/bin/python"
      ]
    },
    "cat": {
      "description": "Concatenate and display files",
      "category": "file_viewing_editing",
      "usage": "cat [OPTIONS] [FILE...]",
      "options": {
        "-n": "Number all output lines",
        "-b": "Number non-blank lines",
        "-s": "Squeeze multiple blank lines",
        "-A": "Show all characters including non-printing",
        "-E": "Display $ at end of each line",
        "-T": "Display TAB characters as ^I"
      },
      "examples": [
        "cat file.txt",
        "cat file1.txt file2.txt",
        "cat -n file.txt",
        "cat file1.txt file2.txt > combined.txt"
      ]
    },
    "head": {
      "description": "Output first part of files",
      "category": "file_viewing_editing",
      "usage": "head [OPTIONS] [FILE...]",
      "options": {
        "-n NUM": "Print first NUM lines (default 10)",
        "-c NUM": "Print first NUM bytes",
        "-q": "Never print headers",
        "-v": "Always print headers"
      },
      "examples": [
        "head file.txt",
        "head -n 20 file.txt",
        "head -c 100 file.txt"
      ]
    },
    "tail": {
      "description": "Output last part of files",
      "category": "file_viewing_editing",
      "usage": "tail [OPTIONS] [FILE...]",
      "options": {
        "-n NUM": "Print last NUM lines (default 10)",
        "-c NUM": "Print last NUM bytes",
        "-f": "Follow file growth",
        "-F": "Follow and retry if file is inaccessible",
        "-q": "Never print headers",
        "-v": "Always print headers"
      },
      "examples": [
        "tail file.txt",
        "tail -n 50 /var/log/syslog",
        "tail -f /var/log/syslog",
        "tail -F application.log"
      ]
    },
    "less": {
      "description": "View file contents with pagination",
      "category": "file_viewing_editing",
      "usage": "less [OPTIONS] [FILE]",
      "options": {
        "-N": "Show line numbers",
        "-S": "Chop long lines",
        "-i": "Ignore case in searches",
        "-X": "No screen clearing on exit"
      },
      "controls": {
        "Space": "Next page",
        "b": "Previous page",
        "q": "Quit",
        "/pattern": "Search forward",
        "?pattern": "Search backward",
        "n": "Next search result",
        "N": "Previous search result",
        "g": "Go to beginning",
        "G": "Go to end"
      },
      "examples": [
        "less file.txt",
        "less -N file.txt"
      ]
    },
    "nano": {
      "description": "Simple text editor",
      "category": "file_viewing_editing",
      "usage": "nano [OPTIONS] [FILE]",
      "options": {
        "-l": "Show line numbers",
        "-m": "Enable mouse support",
        "-i": "Auto-indent new lines",
        "-w": "Disable line wrapping"
      },
      "controls": {
        "^G": "Get Help",
        "^O": "Save (WriteOut)",
        "^X": "Exit",
        "^K": "Cut line",
        "^U": "Paste",
        "^W": "Search",
        "^\\": "Replace",
        "^V": "Next page",
        "^Y": "Previous page"
      },
      "examples": [
        "nano file.txt",
        "nano",
        "nano -l script.sh"
      ]
    },
    "vim": {
      "description": "Vi IMproved text editor",
      "category": "file_viewing_editing",
      "usage": "vim [OPTIONS] [FILE]",
      "options": {
        "+NUM": "Start at line NUM",
        "-R": "Read-only mode",
        "-n": "No swap file"
      },
      "modes": {
        "normal": "Press ESC - navigation and commands",
        "insert": "Press i - text input",
        "visual": "Press v - text selection",
        "command": "Press : - execute commands"
      },
      "basic_commands": {
        ":w": "Save",
        ":q": "Quit",
        ":wq": "Save and quit",
        ":q!": "Quit without saving",
        ":e FILE": "Edit file",
        "i": "Enter insert mode",
        "a": "Append after cursor",
        "o": "Open new line below",
        "ESC": "Return to normal mode",
        "dd": "Delete line",
        "yy": "Copy line",
        "p": "Paste",
        "u": "Undo",
        "/pattern": "Search forward",
        "n": "Next search result"
      },
      "examples": [
        "vim file.txt",
        "vim +10 file.txt",
        "vim -R file.txt"
      ]
    },
    "echo": {
      "description": "Display text or write to files",
      "category": "file_viewing_editing",
      "usage": "echo [OPTIONS] [STRING...]",
      "options": {
        "-n": "Do not output trailing newline",
        "-e": "Enable interpretation of backslash escapes",
        "-E": "Disable backslash escapes (default)"
      },
      "redirection": {
        ">": "Overwrite file",
        ">>": "Append to file"
      },
      "examples": [
        "echo Hello World",
        "echo 'text' > file.txt",
        "echo 'more text' >> file.txt",
        "echo $HOME",
        "echo -e 'Line1\\nLine2'"
      ]
    },
    "find": {
      "description": "Search for files in directory hierarchy",
      "category": "file_search_manipulation",
      "usage": "find [PATH] [OPTIONS]",
      "options": {
        "-name PATTERN": "Search by name (case-sensitive)",
        "-iname PATTERN": "Search by name (case-insensitive)",
        "-type [f|d|l]": "File type (f=file, d=directory, l=symlink)",
        "-size [+|-]SIZE": "Search by size",
        "-mtime DAYS": "Modified in last DAYS days",
        "-user USER": "Files owned by USER",
        "-perm MODE": "Files with permissions MODE",
        "-exec CMD {} \\;": "Execute command on results",
        "-delete": "Delete found files"
      },
      "examples": [
        "find . -name '*.txt'",
        "find /home -type f -name 'config*'",
        "find . -size +1M",
        "find . -mtime -7",
        "find . -name '*.tmp' -delete",
        "find . -type f -exec chmod 644 {} \\;"
      ]
    },
    "grep": {
      "description": "Search text patterns in files",
      "category": "file_search_manipulation",
      "usage": "grep [OPTIONS] PATTERN [FILE...]",
      "options": {
        "-i": "Ignore case",
        "-r": "Recursive search",
        "-n": "Show line numbers",
        "-v": "Invert match",
        "-l": "Show only filenames",
        "-c": "Count matches",
        "-w": "Match whole words",
        "-A NUM": "Show NUM lines after match",
        "-B NUM": "Show NUM lines before match",
        "-C NUM": "Show NUM lines before and after",
        "-E": "Extended regex",
        "-F": "Fixed strings (no regex)"
      },
      "examples": [
        "grep 'pattern' file.txt",
        "grep -r 'TODO' .",
        "grep -i 'error' /var/log/syslog",
        "grep -n 'function' script.js",
        "grep -C 3 'error' log.txt"
      ]
    },
    "sed": {
      "description": "Stream editor for text manipulation",
      "category": "file_search_manipulation",
      "usage": "sed [OPTIONS] 'SCRIPT' [FILE...]",
      "options": {
        "-i": "Edit files in-place",
        "-e SCRIPT": "Add script",
        "-n": "Suppress automatic printing"
      },
      "common_operations": {
        "s/OLD/NEW/": "Substitute first occurrence",
        "s/OLD/NEW/g": "Substitute all occurrences",
        "s/OLD/NEW/i": "Case-insensitive substitution",
        "d": "Delete line",
        "p": "Print line",
        "NUM,NUMd": "Delete lines NUM to NUM"
      },
      "examples": [
        "sed 's/old/new/g' file.txt",
        "sed '1,10d' file.txt",
        "sed -i 's/foo/bar/g' file.txt",
        "sed -n '10,20p' file.txt"
      ]
    },
    "awk": {
      "description": "Pattern scanning and text processing",
      "category": "file_search_manipulation",
      "usage": "awk [OPTIONS] 'PROGRAM' [FILE...]",
      "options": {
        "-F CHAR": "Set field separator",
        "-v VAR=VALUE": "Set variable"
      },
      "built_in_variables": {
        "$0": "Entire line",
        "$1, $2, ...": "Field 1, 2, etc.",
        "NF": "Number of fields",
        "NR": "Line number",
        "FS": "Field separator",
        "OFS": "Output field separator"
      },
      "examples": [
        "awk '{print $1}' file.txt",
        "awk -F: '{print $1,$3}' /etc/passwd",
        "awk '/pattern/ {print $0}' file.txt",
        "awk 'NR==10' file.txt"
      ]
    },
    "wc": {
      "description": "Word, line, character, and byte count",
      "category": "file_search_manipulation",
      "usage": "wc [OPTIONS] [FILE...]",
      "options": {
        "-l": "Count lines",
        "-w": "Count words",
        "-c": "Count bytes",
        "-m": "Count characters",
        "-L": "Print length of longest line"
      },
      "examples": [
        "wc file.txt",
        "wc -l file.txt",
        "ls | wc -l",
        "wc -w document.txt"
      ]
    },
    "sort": {
      "description": "Sort lines of text",
      "category": "file_search_manipulation",
      "usage": "sort [OPTIONS] [FILE...]",
      "options": {
        "-r": "Reverse sort",
        "-n": "Numeric sort",
        "-k NUM": "Sort by column NUM",
        "-u": "Remove duplicates",
        "-t CHAR": "Use CHAR as field separator",
        "-h": "Human-readable numbers",
        "-f": "Ignore case"
      },
      "examples": [
        "sort file.txt",
        "sort -r file.txt",
        "sort -n numbers.txt",
        "sort -k2 data.txt",
        "sort -u file.txt"
      ]
    },
    "uniq": {
      "description": "Report or omit repeated lines",
      "category": "file_search_manipulation",
      "usage": "uniq [OPTIONS] [FILE]",
      "options": {
        "-c": "Prefix lines with count",
        "-d": "Only print duplicates",
        "-u": "Only print unique lines",
        "-i": "Ignore case",
        "-f NUM": "Skip NUM fields"
      },
      "examples": [
        "sort file.txt | uniq",
        "sort file.txt | uniq -c",
        "sort file.txt | uniq -d"
      ]
    },
    "diff": {
      "description": "Compare files line by line",
      "category": "file_search_manipulation",
      "usage": "diff [OPTIONS] FILE1 FILE2",
      "options": {
        "-u": "Unified format",
        "-c": "Context format",
        "-y": "Side-by-side comparison",
        "-i": "Ignore case",
        "-w": "Ignore whitespace",
        "-b": "Ignore space changes",
        "-r": "Recursively compare directories",
        "-q": "Report only when files differ"
      },
      "examples": [
        "diff file1.txt file2.txt",
        "diff -u old.txt new.txt",
        "diff -y file1.txt file2.txt",
        "diff -r dir1/ dir2/"
      ]
    },
    "chmod": {
      "description": "Change file permissions",
      "category": "file_permissions_ownership",
      "usage": "chmod [OPTIONS] MODE FILE...",
      "options": {
        "-R": "Recursive",
        "-v": "Verbose output",
        "-c": "Report only when change is made"
      },
      "numeric_modes": {
        "0": "---",
        "1": "--x",
        "2": "-w-",
        "3": "-wx",
        "4": "r--",
        "5": "r-x",
        "6": "rw-",
        "7": "rwx"
      },
      "symbolic_modes": {
        "u": "User/owner",
        "g": "Group",
        "o": "Others",
        "a": "All",
        "+": "Add permission",
        "-": "Remove permission",
        "=": "Set exact permission"
      },
      "examples": [
        "chmod 755 script.sh",
        "chmod u+x script.sh",
        "chmod -R 644 *.txt",
        "chmod go-w file.txt",
        "chmod a+r file.txt"
      ]
    },
    "chown": {
      "description": "Change file owner and group",
      "category": "file_permissions_ownership",
      "usage": "chown [OPTIONS] [OWNER][:GROUP] FILE...",
      "options": {
        "-R": "Recursive",
        "-v": "Verbose output",
        "-c": "Report only when change is made",
        "--reference=FILE": "Use FILE's owner/group"
      },
      "examples": [
        "chown user file.txt",
        "chown user:user file.txt",
        "chown -R user:user folder/",
        "chown :group file.txt"
      ]
    },
    "tar": {
      "description": "Archive files",
      "category": "compression_archiving",
      "usage": "tar [OPTIONS] [ARCHIVE] [FILE...]",
      "options": {
        "-c": "Create archive",
        "-x": "Extract archive",
        "-t": "List contents",
        "-f": "Specify filename",
        "-v": "Verbose",
        "-z": "Gzip compression (.tar.gz)",
        "-j": "Bzip2 compression (.tar.bz2)",
        "-J": "Xz compression (.tar.xz)",
        "-C DIR": "Change to directory"
      },
      "examples": [
        "tar -czf archive.tar.gz folder/",
        "tar -xzf archive.tar.gz",
        "tar -tzf archive.tar.gz",
        "tar -xzf archive.tar.gz -C /tmp/",
        "tar -cjf archive.tar.bz2 folder/"
      ]
    },
    "gzip": {
      "description": "Compress files",
      "category": "compression_archiving",
      "usage": "gzip [OPTIONS] [FILE...]",
      "options": {
        "-d": "Decompress",
        "-k": "Keep original file",
        "-r": "Recursive",
        "-v": "Verbose",
        "-NUM": "Compression level (1-9)",
        "-t": "Test compressed file"
      },
      "examples": [
        "gzip file.txt",
        "gzip -d file.txt.gz",
        "gzip -k file.txt",
        "gzip -9 file.txt"
      ]
    },
    "gunzip": {
      "description": "Decompress gzip files",
      "category": "compression_archiving",
      "usage": "gunzip [OPTIONS] [FILE...]",
      "options": {
        "-k": "Keep original file",
        "-v": "Verbose",
        "-t": "Test compressed file"
      },
      "examples": [
        "gunzip file.txt.gz",
        "gunzip -k file.txt.gz"
      ]
    },
    "zip": {
      "description": "Package and compress files",
      "category": "compression_archiving",
      "usage": "zip [OPTIONS] ARCHIVE FILE...",
      "options": {
        "-r": "Recursive",
        "-q": "Quiet mode",
        "-v": "Verbose",
        "-NUM": "Compression level (0-9)",
        "-e": "Encrypt",
        "-u": "Update existing archive"
      },
      "examples": [
        "zip archive.zip file.txt",
        "zip -r archive.zip folder/",
        "zip -9 archive.zip file.txt"
      ]
    },
    "unzip": {
      "description": "Extract zip archives",
      "category": "compression_archiving",
      "usage": "unzip [OPTIONS] ARCHIVE",
      "options": {
        "-l": "List contents",
        "-d DIR": "Extract to directory",
        "-q": "Quiet mode",
        "-o": "Overwrite without prompting",
        "-n": "Never overwrite",
        "-v": "Verbose"
      },
      "examples": [
        "unzip archive.zip",
        "unzip archive.zip -d folder/",
        "unzip -l archive.zip"
      ]
    },
    "uname": {
      "description": "Print system information",
      "category": "system_information",
      "usage": "uname [OPTIONS]",
      "options": {
        "-a": "All information",
        "-s": "Kernel name",
        "-n": "Hostname",
        "-r": "Kernel release",
        "-v": "Kernel version",
        "-m": "Machine hardware",
        "-p": "Processor type",
        "-i": "Hardware platform",
        "-o": "Operating system"
      },
      "examples": [
        "uname",
        "uname -a",
        "uname -r"
      ]
    },
    "hostname": {
      "description": "Show or set system hostname",
      "category": "system_information",
      "usage": "hostname [OPTIONS] [NAME]",
      "options": {
        "-f": "Fully qualified domain name",
        "-i": "IP address",
        "-I": "All IP addresses",
        "-s": "Short hostname"
      },
      "examples": [
        "hostname",
        "hostname -f",
        "hostname -I"
      ]
    },
    "whoami": {
      "description": "Print current username",
      "category": "system_information",
      "usage": "whoami",
      "examples": ["whoami"]
    },
    "id": {
      "description": "Print user identity",
      "category": "system_information",
      "usage": "id [OPTIONS] [USER]",
      "options": {
        "-u": "User ID only",
        "-g": "Group ID only",
        "-G": "All group IDs",
        "-n": "Name instead of number",
        "-r": "Real ID instead of effective"
      },
      "examples": [
        "id",
        "id user",
        "id -u",
        "id -gn"
      ]
    },
    "uptime": {
      "description": "Show system uptime and load",
      "category": "system_information",
      "usage": "uptime [OPTIONS]",
      "options": {
        "-p": "Pretty format",
        "-s": "System up since"
      },
      "examples": [
        "uptime",
        "uptime -p"
      ]
    },
    "date": {
      "description": "Display or set system date and time",
      "category": "system_information",
      "usage": "date [OPTIONS] [+FORMAT]",
